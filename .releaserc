{
	"branches": [
		"main"
	],
	"repositoryUrl": "https://github.com/ryan-voitiskis/astro-helmet.git",
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "angular",
				"releaseRules": [
					{
						"type": "chore",
						"release": "patch"
					}
				]
			}
		],
		"@semantic-release/release-notes-generator",
		"@semantic-release/npm",
		"@semantic-release/github"
	]
}
